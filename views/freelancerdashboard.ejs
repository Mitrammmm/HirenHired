<!-- views/freelancerDashboard.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Freelancer Dashboard</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
  <div class="container mt-5">
    <h1>Welcome, </h1>
    
    <div class="row">
      <!-- Freelancer Info -->
      <div class="col-md-4">
        <img src="" alt="Avatar" class="img-fluid rounded-circle">
        <h3></h3>
        <p><strong>Email:</strong> </p>
        <p><strong>Location:</strong> </p>
      </div>
      
      <!-- Work in Hand Section -->
      <div class="col-md-8">
        <h3>Work in Hand</h3>
        <table class="table table-bordered">
          <thead>
            <tr>
              <th>Title</th>
              <th>Client Name</th>
              <th>Status</th>
              <th>Deadline</th>
              <th>Earnings</th>
            </tr>
          </thead>
          <tbody>
            
          </tbody>
        </table>
        
        <!-- Button to view analytics in Google Data Studio -->
        <h3 class="mt-5">Work Analytics</h3>
        <iframe width="100%" height="600" src="https://datastudio.google.com/embed/reporting/<YOUR_GOOGLE_STUDIO_REPORT_ID>/page/1" frameborder="0" style="border:0" allowfullscreen></iframe>
      </div>
    </div>
  </div>
</body>
</html>
